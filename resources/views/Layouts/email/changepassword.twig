 {% block content %}
     {% set body %}
         <div>
             <form action="/email/manage" method="post">
                 <div style="padding-bottom: 20px"></div>
                 {%  include 'template/inputs.twig' with  {'inpid' :  'upc1' , 'inptype': 'password','labid':'upcl1' ,'lab':'password','inpname':'pass','validate':true , 'required' :true ,'tip_data' : 'Password Must Contains atleast one letter , one number , one symbol and have minimum 10 charcters length ','tip_dir' :'right' } %}
                 <div style="padding-top: 10px;padding-bottom: 10px"></div>
                 {%  include 'template/inputs.twig' with  {'inpid' :  'upc2' , 'inptype': 'password','labid':'upcl2' ,'lab':'Confirm Password','inpname':'conf_pass','validate':true , 'required' :true ,'tip_data' : 'Password Must Contains atleast one letter , one number , one symbol and have minimum 10 charcters length ','tip_dir' :'right'  } %}
                 <div style="padding-top: 10px;padding-bottom: 10px"></div>
                 <input type="hidden" value="" name="email" id="pass">
                 <button name="change_password" type="submit" class="button blue" >Change Email Account Password</button>
             </form>
         </div>
     {% endset %}
     {% set model_body ={'model_body' :  body  } %}
     {%  include'template/model.twig' with  {'model_body' :  body , 'model_id': 'change_pass' } %}
{% endblock %}